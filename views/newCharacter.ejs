<body>
    <h1>Enter a new StarWars Character</h1>
    <form method="post" action="/newCharacter">
        <div>
            <label for="name">Name:</label>
            <input type="text" name="name">
        </div>
        <div>
            <label for="height">Name:</label>
            <input type="text" name="height">
        </div>
        <div>
            <label for="mass">Name:</label>
            <input type="text" name="mass">
        </div>
        <div>
            <button type="submit">Add a new Character</button>
        </div>
    </form>

</body>

